{
  "name": "lec-electron",
  "description": "electron lecture app",
  "version": "1.0.0",
  "author": "gondr",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "electron .",
    "watch": "webpack --watch",
    "x64build": "electron-packager . lecshare --platform=win32 --arch=x64 --out=dist64 --overwrite",
    "x32build": "electron-packager . lecshare --platform=win32 --arch=ia32 --out=dist32 --overwrite"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "dom-to-image": "^2.6.0",
    "ejs": "^2.6.1",
    "electron": "^4.0.8",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fs": "0.0.1-security",
    "hawk": "^7.0.10",
    "highlight.js": "^9.15.6",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "socket.io": "^2.2.0",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "asar": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^13.1.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.4",
    "jshint": "^2.10.1",
    "vue-loader": "^13.0.5",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.2.1"
  }
}
